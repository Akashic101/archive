---
title: Site Files
layout: base.njk
---

<h2>1950's</h2>

{% set filteredFiles = collections.fileList | filterByDecade(1950) %}

{% if filteredFiles.length > 0 %}
  <table>
  <tr>
    <th>Name</th>
    <th>Extension</th>
    <th>Path</th>
    <th>Created</th>
    <th>Last modified</th>
  </tr>
  {% for file in filteredFiles %}  
    <tr>
      <td><a href="{{ file.path }}" target="_blank">{{ file.name }}</a></td>
      <td>{{ file.extension }}</td>
      <td>{{ file.path }}</td>
      <td>{{ file.created | localDate }}</td>
      <td>{{ file.modified | localDate }}</td>
    </tr>
  {% endfor %}
</table>  
{% else %}
  <p>No file data to display</p>
{% endif %}

